{
  "name": "pegboard-board-hook",
  "version": "1.0.1",
  "description": "Snowboard and skateboard hooks for 1-inch pegboard",
  "main": "index.js",
  "author": "Will Duff",
  "license": "MIT",
  "dependencies": {
    "@jscad/cli": "^2.3.5",
